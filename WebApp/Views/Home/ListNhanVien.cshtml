
@model List<DAL.Models.Nhanvien>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.9.1/font/bootstrap-icons.css">
<table class="table">
      <thead>
        <tr style="background-image:url(data:image/jpeg;base64,)">
              <th>
                <h4 class="text-info"> Danh sách nhân viên</h4>
              </th>
            <th>
                <a asp-controller="Home" asp-action="ShowCreate" class="btn btn-success">Thêm mới</a>
              </th>
          </tr>
      </thead>
  </table> 
<table class="table">
  <thead>
        <tr style="background:Gray;">
      <th scope="col">#</th>
      <th scope="col">Họ tên</th>
      <th scope="col">Số điện thoại</th>
      <th scope="col">Tài khoản</th>
      <th> </th>
      <th> </th>
    </tr>
  </thead>
  <tbody>
        @foreach (var nv in Model)
        {
            <tr style="background:#80bfff">
                <td>@nv.MaNv</td>
                <td>@nv.TenNv</td>
                <td>@nv.Sdt</td>
                <td>@nv.TaiKhoan</td>
                 <td>
                    <a asp-controller="Home" asp-action="ShowUpdate" asp-route-id="@nv.MaNv"><i class="bi bi-pencil"></i> </a>                   
                </td>
                <td>
                    <a asp-controller="Home" asp-action="Delete" asp-route-id="@nv.MaNv"><i class="bi bi-trash"></i> </a>
                </td>
               
            </tr>
        }
  </tbody>
</table>